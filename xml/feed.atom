---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>psychedeli.ca</title>
  <link href="http://psychedeli.ca/"/>
  <link type="application/atom+xml" rel="self" href="http://psychedeli.ca/xml/feed.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://coyled.com/</id>
  <author>
    <name>Tom Scott</name>
    <email>tubbo@psychedeli.ca</email>
  </author>

  {% for post in site.posts %}
	  <entry>
	    <id>http://psychedeli.ca{{ post.id }}</id>
	    <link type="text/html" rel="alternate" href="http://psychedeli.ca{{ post.url }}"/>
	    <title>{{ post.title }}</title>
	    <updated>{{ post.date | date_to_xmlschema }}</updated>
	    <author>
	      <name>Tom Scott</name>
	      <uri>http://psychedeli.ca/</uri>
	    </author>
	    <content type="html">{{ post.content | xml_escape }}</content>
	  </entry>
  {% endfor %}
 
</feed>
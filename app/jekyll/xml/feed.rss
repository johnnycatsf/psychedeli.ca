---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>psychedeli.ca</title>
    <link>http://psychedeli.ca/</link>
    <atom:link href="http://psychedeli.cam/xml/feed.rss" rel="self" type="application/rss+xml" />
    <description>a blog about music composition/gormandization and programming.</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %Z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %Z" }}</lastBuildDate>

    {% for post in site.posts %}
	    <item>
	      <title>{{ post.title }}</title>
	      <link>http://psychedeli.ca{{ post.url }}</link>
	      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %Z" }}</pubDate>
	      <author>tubbo@psychedeli.ca (Tom Scott)</author>
	      <guid>http://psychedeli.ca{{ post.id }}</guid>
	      <description>{{ post.content | xml_escape }}</description>
	    </item>
    {% endfor %}

  </channel>
</rss>
